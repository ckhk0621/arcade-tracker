{
  "name": "arcade-tracker",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev -p 4400",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev -p 4400",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "import:jumping-gym": "cross-env NODE_OPTIONS=--no-deprecation node scripts/import-jumping-gym.js",
    "import:jumping-gym:status": "cross-env NODE_OPTIONS=--no-deprecation node scripts/import-jumping-gym.js status",
    "import:jumping-gym:process": "cross-env NODE_OPTIONS=--no-deprecation node scripts/import-jumping-gym.js process"
  },
  "dependencies": {
    "@payloadcms/db-mongodb": "3.49.1",
    "@payloadcms/next": "3.49.1",
    "@payloadcms/payload-cloud": "3.49.1",
    "@payloadcms/richtext-lexical": "3.49.1",
    "@payloadcms/ui": "3.49.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/leaflet": "^1.9.20",
    "autoprefixer": "^10.4.21",
    "cheerio": "1.0.0",
    "cloudinary": "^2.7.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.11.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.535.0",
    "next": "15.4.4",
    "node-fetch": "^3.3.2",
    "payload": "3.49.1",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-dropzone": "^14.3.8",
    "react-leaflet": "^5.0.0",
    "sharp": "0.34.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.17.0",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.4",
    "jsdom": "26.1.0",
    "playwright": "1.54.1",
    "playwright-core": "1.54.1",
    "prettier": "^3.6.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
